FUNCTION insertionSort(VAR arr  ARRAY_OF INTEGER) : ARRAY_OF INTEGER
VAR
    i, j, key : INTEGER;
BEGIN
    FOR i FROM 1 TO arr.legth-1 STEP 1 DO
        key := arr[i];
        j := j-1;
        WHILE (j >= 0 and arr[j] key) DO
            arr[j+1] := arr[j];
            j := j-1;
        END_WHILE
        arr[j+1] := key;
    END_FOR
    RETURN value ;
END